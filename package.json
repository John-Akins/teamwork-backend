{
  "name": "teamwork-backend",
  "version": "1.0.0",
  "description": "secure Backend RESTful services",
  "main": "index.js",
  "engines": {
    "node": "10.16.0",
    "npm": "6.9.0"
  },
  "scripts": {
    "start": "set NODE_ENV=development &&  node build/src/server.js",
    "build": "rm -rf ./build && babel -d build/src ./src -s",
    "dev": "set NODE_ENV=development && nodemon --exec babel-node src/server.js",
    "test": "set NODE_ENV=test && nyc --require @babel/register  mocha ./src/test/test-j--create-gif.js --timeout 40000 --exit",
    "generate-lcov": "nyc report --reporter=text-lcov > lcov.info",
    "coveralls-coverage": "coveralls < lcov.info",
    "coverage": "npm run generate-lcov && npm run coveralls-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/John-Akins/teamwork-backend.git"
  },
  "author": "John-Akins",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/John-Akins/teamwork-backend/issues"
  },
  "homepage": "https://github.com/John-Akins/teamwork-backend#readme",
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/register": "^7.7.0",
    "@babel/runtime": "^7.7.4",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "^3.0.7",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "webpack": "^4.41.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.4",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.17.0",
    "cool-ascii-faces": "^1.3.4",
    "datauri": "^2.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-sanitizer": "^1.0.5",
    "express-validator": "^6.2.0",
